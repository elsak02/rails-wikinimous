<div class="container">
<h1>My articles</h1>

<ul class="article-list list-group">
  <% @articles.each do |article| %>
    <li class="list-group-item"><%=link_to "#{article.title}", article_path(article)%></li>
  <% end %>
</ul>

<br>

<%=link_to "Add an article", new_article_path, class: "btn btn-primary"%>

</div>
